{
    "source": {
        "include": [
            "dist",
            "config/net-server.js"
        ],
        "exclude": [
            "node_modules"
        ],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./docs/",
        "recurse": true
    }
}